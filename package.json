{
  "name": "test",
  "version": "2.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "tests": "jest --watch",
    "start": "node index.js",
    "watch": "tsc -w",
    "serve": "nodemon -e ts --watch src --exec \"npm run build && npm run start\" ",
    "types": "types-installer install --toDev"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^4.0.2",
    "@nestjs/core": "^4.0.2",
    "@nestjs/graphql": "^2.0.0",
    "@nestjs/microservices": "^4.0.2",
    "@nestjs/testing": "^4.0.2",
    "@nestjs/websockets": "^4.0.2",
    "apollo-server-express": "^1.3.2",
    "bluebird": "^3.5.1",
    "graphql": "^0.13.0",
    "graphql-tools": "^2.21.0",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "@types/": "nestjs/graphql",
    "@types/bluebird": "^3.5.20",
    "@types/chai": "^4.1.2",
    "@types/express": "^4.0.36",
    "@types/graphql": "^0.12.3",
    "@types/jest": "^22.1.2",
    "@types/node": "^8.0.19",
    "@types/ramda": "^0.25.17",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sinon": "^4.1.3",
    "@types/typescript": "^2.0.0",
    "chai": "^4.1.1",
    "jest": "^22.2.2",
    "nodemon": "^1.11.0",
    "sinon": "^2.4.1",
    "ts-jest": "^22.0.4",
    "ts-node": "^3.3.0",
    "types-installer": "^1.4.0",
    "typescript": "^2.4.2"
  },
  "main": "index.js",
  "author": "Rasmus Vermeulen <vermeulen.rasmus@gmail.com>"
}
